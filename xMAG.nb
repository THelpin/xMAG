    RowBox[{
     RowBox[{"ConformalTransform", "::", "usage"}], "=", 
     "\"\<ConformalTransform[exp,g,gc] apply the conformal rules from metric \
g to metric gc to exp.\>\""}], ";"}], "\n", 
