              RowBox[{"Expand", "@", 
               RowBox[{"VarDt", "[", 
                RowBox[{"metric", ",", "expr"}], "]"}]}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"novar", "=", 
              RowBox[{"mod", "/.", 
